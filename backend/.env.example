# Backend Environment Variables
# Copy this file to .env before running the application.

# Server Port
# Default: 5000
PORT=5000

# MongoDB Connection String
# Example: mongodb+srv://<username>:<password>@cluster.mongodb.net/dynaqr
MONGO_URI=mongodb://localhost:27017/dynaqr

# JWT Secret Key
# REQUIRED: Must be at least 32 characters long, random, and unguessable.
# DO NOT use the default value below in production.
JWT_SECRET=CHANGE_THIS_TO_A_LONG_RANDOM_STRING_MIN_32_CHARS