# Backend Environment Variables
# Copy this file to .env before running the application.

# Server Port
# Default: 5000
PORT=5000

# MongoDB Connection String
# Example: mongodb+srv://<username>:<password>@cluster.mongodb.net/dynaqr
MONGO_URI=mongodb://localhost:27017/dynaqr

# JWT Secret Key
# Used for signing authentication tokens.
# Change this to a secure random string in production.
JWT_SECRET=development_secret_key_123
